<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <!-- Facebook Open Graph tags -->
    <meta property="og:image" content="<%= image_tag 'fleur2.jpg' %>"/>
    <meta property="og:site_name" content="Fleurs d'Antony" />
    <meta property="og:type" content=" Fleuriste à Antony, nous vous attendons 7/7" />
    <meta property="og:url" content="http://www.fleursdantony.com" />
    <meta property="og:title" content="" />

    <% set_meta_tags title: 'Fleurs dAntony, Fleuriste à Antony' %>


    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Fleurs d'Antony" %></title>

    <!-- For Bootstrap responsiveness -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <%= partial 'layouts/favicon' %>


    <%= stylesheet_link_tag "all" %>
    <%= javascript_include_tag  "all" %>
  </head>


 <%= partial 'layouts/nav' %>

  <body class="<%= page_classes %>">
    <%= yield %>
  </body>

    <!-- Footer -->
  <%= partial 'layouts/footer' %>

  </html>
